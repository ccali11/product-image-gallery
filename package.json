{
  "name": "product-image-gallery",
  "version": "1.0.0",
  "description": "Displays images from database sorted by setting, metal, stone cut, and stone carat.",
  "main": "./server/index.js",
  "jest": {
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/db/",
      "/server/"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "scripts": {
    "start": "nodemon ./server/index.js",
    "test": "jest --detectOpenHandles",
    "build": "webpack --config ./webpack.config.js --watch",
    "seed": "mysql -u root < ./db/db.sql && node ./db/seed-db/seed.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Edgar-Allan-Crows/product-image-gallery.git"
  },
  "author": "CSellers720",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Edgar-Allan-Crows/product-image-gallery/issues"
  },
  "homepage": "https://github.com/Edgar-Allan-Crows/product-image-gallery#readme",
  "dependencies": {
    "axios": "^0.20.0",
    "express": "^4.17.1",
    "moment": "^2.29.0",
    "mysql2": "^2.2.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-image-magnifiers": "^1.4.0",
    "react-indiana-drag-scroll": "^1.7.0",
    "react-star-ratings": "^2.3.0",
    "sequelize": "^6.3.5",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "babel-jest": "^26.3.0",
    "cors": "^2.8.5",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "faker": "^5.1.0",
    "jest": "^26.4.2",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.2",
    "react-test-renderer": "^16.13.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev": "^1.1.1"
  }
}
